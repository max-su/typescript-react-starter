{
  "name": "typescript-react-starter",
  "version": "0.0.1",
  "description": "A more minimal starter template for typescript/react",
  "repository": "git@github.com:max-su/typescript-react-starter.git",
  "author": "max@maxsu.dev",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/webpack/bin/webpack.js --mode=development --config ./webpack.config.js && ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack.config.js --inline --content-base ./build",
    "build": "./node_modules/webpack/bin/webpack.js --mode=production --config ./webpack.config.js",
    "test": "./node_modules/jest/bin/jest.js",
    "update-snapshots": "./node_modules/jest/bin/jest.js --updateSnapshot"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.1.2",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
